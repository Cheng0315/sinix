<div class="user-card-container">
  <div class="jumbotron profile-page">
    <h2 style="margin:auto;" class="text-center"><i class="fas fa-user fa-3x"></i></h2>
    <br><br>
    <!-- Display user's name-->
    <h6 class="text-center">Name</h6>
    <h3 class="text-center"><%=current_user.first_name%>&nbsp;<%=current_user.last_name%></h3>
    <br><br>
    <!-- Display user's email-->
    <h6 class="text-center">Email</h6>
    <h3 class="text-center"><%=current_user.email%></h3>
  </div>
</div>
